---
import type { GetStaticPaths } from "astro";
import { languages } from "../../i18n/ui";

import ContactUs from "../../components/contactUs.astro";
import Features from "../../components/features.astro";
import HomeHeader from "../../components/headers/homeHeader.astro";
import Services from "../../components/services.astro";
import Statistics from "../../components/statistics.astro";
import Steps from "../../components/steps.astro";
import Layout from "../../layouts/Layout.astro";
import LatestPost from "../../components/latestPost.astro";

export const getStaticPaths = (() => {
	return Object.keys(languages).map( locale => ({ 
		params : { locale }
	}));
}) satisfies GetStaticPaths;

---

<Layout>
	
	<HomeHeader />

	<Features />
	
	<Services />

	<Steps />

	<Statistics />
	
	<LatestPost />
	
	<ContactUs />

</Layout>